def apply_operations(nums):
    n = len(nums)
    for i in range(n - 1):
        if nums[i] == nums[i + 1]:
            nums[i] *= 2
            nums[i + 1] = 0
    
    result = [num for num in nums if num != 0] + [0] * nums.count(0)
    return result
nums1 = [1,2,2,1,1,0]
nums2 = [0,1]
print(apply_operations(nums1))  
print(apply_operations(nums2))  

